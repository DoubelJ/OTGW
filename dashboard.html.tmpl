<!DOCTYPE html>
<html lang="en">
<head>
[include header.inc]	
</head>
<body>
[include navbar.inc]

<!-- Page Content -->
<div class="container-fluid">
    <!-- Marketing Icons Section -->
    <div class="row">
        <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Room temperature</span>
                </div>
                <div class="panel-body panelValue" id="temperature"></div>
            </div>
        </div>
        <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Outside temperature</span>
                </div>
                <div class="panel-body panelValue" id="outside"></div>   
            </div>
        </div>          
        <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Room setpoint</span>
                </div>
                <div class="panel-body panelValue" id="setpoint"> </div>
            </div>
        </div>
        <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Boiler water temperature</span>
                </div>
                <div class="panel-body panelValue" id="boilertemp"></div>
            </div>
        </div>      
    </div>
    
    <div class="row">   
        <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Remote override room setpoint</span>
                </div>
                <div class="panel-body panelValue" id="override"></div>
            </div>
        </div>        
        <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Return water temperature</span>
                </div>
                <div class="panel-body panelValue" id="returntemp"></div>
            </div>
        </div>     
        <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Control setpoint</span>
                </div>
                <div class="panel-body panelValue" id="controlsp"></div>
            </div>
        </div>   
         <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Domestic hot water temperature</span>
                </div>
                <div class="panel-body panelValue" id="dhwtemp"></div>
            </div>
        </div>             
    </div>
    
    <div class="row">   
         <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Max. central heating water setpoint</span>
                </div>
                <div class="panel-body panelValue" id="chwsetpoint"></div>
            </div>
        </div>    
         <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Domestic hot water setpoint</span>
                </div>
                <div class="panel-body panelValue" id="dhwsetpoint"></div>
            </div>
        </div>         
         <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Relative modulation level</span>
                </div>
                <div class="panel-body panelValue" id="modulation"></div>
            </div>
        </div>      
         <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Central heating water pressure</span>
                </div>
                <div class="panel-body panelValue" id="pressure"></div>
            </div>
        </div>         
    </div>   
    <div class="row">   
         <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>Max. relative modulation level</span>
                </div>
                <div class="panel-body panelValue" id="maxmod"></div>
            </div>
        </div>     
         <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span>OEM fault code</span>
                </div>
                <div class="panel-body panelValue" id="faultcode"></div>
            </div>
        </div>          
 
        
    </div>       
        <!-- /.row -->
</div>

<!-- /.container -->

<script type="text/javascript" src="js/dashboard.js"></script>

</body>
</html>
